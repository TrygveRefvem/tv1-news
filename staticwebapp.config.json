{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/api/*"]
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST"],
      "rewrite": "/api/proxy"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:; connect-src 'self' https://*.azurestaticapps.net https://*.clerk.accounts.dev https://api.worldnewsapi.com https://generativelanguage.googleapis.com",
    "Access-Control-Allow-Origin": "https://red-coast-0699c7710.4.azurestaticapps.net",
    "Access-Control-Allow-Methods": "GET, POST",
    "Access-Control-Allow-Headers": "Content-Type, Authorization",
    "cache-control": "no-cache",
    "X-Content-Type-Options": "nosniff"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}